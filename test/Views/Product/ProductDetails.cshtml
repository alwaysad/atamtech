@model IEnumerable<test.Models.Product>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    
    <title>ProductDetails</title>
</head>

    <table class="table">
        
    
    @foreach (var item in Model) {
        <tr>
            <td>
                item.name
            </td>
            <td>
                item.content
            </td>
            <td>
               item.flightTime
            </td>
            <td>
                item.flightRange
            </td>
            <td>
                item.cruiseSpeed
            </td>
            <td>
                item.payload
            </td>
            <td>
                item.setupTime
            </td>
            <td>
                item.motor
            </td>
            
        </tr>
    }
    
    </table>
</body>
</html>
